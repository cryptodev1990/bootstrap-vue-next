{
  "name": "bootstrap-vue-3-icons",
  "displayName": "BootstrapVue3Icons",
  "description": "A package to incorporate Bootstrap Icons into Bootstrap-vue-3",
  "version": "0.0.2",
  "license": "MIT",
  "main": "./dist/bootstrap-vue-3-icons.umd.js",
  "module": "./dist/bootstrap-vue-3-icons.es.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/bootstrap-vue-3-icons.es.js",
      "require": "./dist/bootstrap-vue-3-icons.umd.js"
    },
    "./dist/bootstrap-vue-3-icons.css": "./dist/bootstrap-vue-3-icons.css",
    "./nuxt": "./nuxt.js",
    "./src/styles/styles.scss": "./src/styles/styles.scss"
  },
  "files": [
    "dist",
    "src",
    "nuxt.js"
  ],
  "bin": {
    "generate": "./scripts/generate.js"
  },
  "types": "./dist/BootstrapVueIcons.d.ts",
  "private": false,
  "scripts": {
    "dev": "vite --port 5175",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "format": "prettier . --write",
    "test": "pnpm run test:lint && pnpm run test:unit",
    "test:lint": "eslint --ext .js,.vue --ignore-path ../../.gitignore --fix src",
    "test:unit": "vitest",
    "test:coverage": "vitest --coverage",
    "buildIcons": "pnpm exec generate",
    "generate": "ts-node-esm --experimental-specifier-resolution=node ./scripts/generate.ts"
  },
  "peerDependencies": {
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.2",
    "vue": "^3.2.45"
  },
  "dependencies": {
    "@nuxt/kit": "3.0.0"
  },
  "devDependencies": {
    "@popperjs/core": "^2.11.6",
    "@types/bootstrap": "^5.2.6",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.10",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitest/coverage-c8": "^0.25.3",
    "@vue/runtime-core": "^3.2.45",
    "@vue/shared": "^3.2.45",
    "@vue/test-utils": "^2.2.5",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.2",
    "c8": "^7.7.0",
    "happy-dom": "^7.7.2",
    "lodash-es": "^4.17.21",
    "rollup": "^3.5.1",
    "rollup-plugin-visualizer": "^5.8.3",
    "sass": "^1.56.1",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-plugin-dts": "^1.7.1",
    "vitest": "^0.25.3",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cdmoro/bootstrap-vue-3.git"
  },
  "bugs": {
    "url": "https://github.com/cdmoro/bootstrap-vue-3/issues"
  },
  "homepage": "https://github.com/cdmoro/bootstrap-vue-3",
  "keywords": [
    "vue3",
    "vue",
    "bootstrap",
    "components",
    "typescript",
    "component-library",
    "icons",
    "bootstrap-vue-3-icons"
  ],
  "lint-staged": {
    "*.{js,vue}": "eslint --cache --fix",
    "*": "prettier --write --ignore-unknown"
  },
  "sideEffects": false
}
